{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "redirects": [
    {
      "source": "/api/post/*/:data",
      "destination": "https://api-proxy-server-steel.vercel.app/api/post/*/:data"
    }
  ]
}
